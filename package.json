    "test": "set API_PORT=3010 && set MODE=prod&& nodemon api/api",
    "start": "set MODE=dev&& netlify-lambda serve api -t 20",
    "build": "./node_modules/.bin/netlify-lambda build api"
  },
  "keywords": [
    "nodejs",
    "react",
    "RESTful",
    "MongoDB"
  ],
