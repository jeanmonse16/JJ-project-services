    "dotenv": "^8.2.0",
    "express": "4.17.1",
    "express-session": "^1.17.1",
    "jsonwebtoken": "8.5.1",
    "mongoose": "^5.12.10",
    "multer": "1.4.2",
    "multer-s3": "^2.9.0",
    "mysql": "2.18.1",
